{
  "name": "justaos",
  "version": "1.0.0",
  "description": "Software Development Kit for JUSTAOS",
  "keywords": [
    "oclif",
    "justaos",
    "JUSTAOS CLI",
    "devkit",
    "sdk",
    "JUSTAOS DevKit"
  ],
  "homepage": "https://github.com/justaos/cli.git",
  "bugs": {
    "url": "https://gitlab.com/justaos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justaos/cli.git"
  },
  "license": "Apache-2.0",
  "author": "Shivaji Varma Pusapati Venkata <contact@shivajivarma.com>",
  "contributors": [],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "bin": {
    "justaos": "../bin/run",
    "justaos-dev": "../bin/dev"
  },
  "files": [
    "bin",
    "lib",
    "resources",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "debug": "./bin/run new test",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "local_publish": "npm install -g .",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^1.6.1",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@justaos/file-utils": "^1.0.3",
    "extract-zip": "^2.0.1",
    "inquirer": "^7.3.3",
    "ora": "^5.3.0",
    "pm2": "^4.5.4",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.0",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/shelljs": "^0.8.8",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "justaos",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
